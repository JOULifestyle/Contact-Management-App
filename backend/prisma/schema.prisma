generator client {
provider = "prisma-client-js"
}


datasource db {
provider = "postgresql"
url = env("DATABASE_URL")
}

model User {
  id        Int       @id @default(autoincrement())
  email     String    @unique
  password  String
  contacts  Contact[]
}


model Contact {
  id        Int    @id @default(autoincrement())
  name      String @unique
  email     String @unique
  phone     String @unique
  category  String?
  birthday  DateTime?
  company   String?
  photoUrl    String?
  userId    Int
  user      User   @relation(fields: [userId], references: [id])
}